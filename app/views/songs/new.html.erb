<%= form_for(@song) do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f[artist_id]label :@artist.id %>
  <%= f[artist_id]text_field :@artist.id %>
  <%= f.genre.id.label :genre.id %>
  <%= f.genre.id.text_field :genre.id %>

  <%= f.submit %>
<% end %>

fill_in 'song[artist_id]', with: @artist.id
fill_in 'song[genre_id]', with: @genre.id

